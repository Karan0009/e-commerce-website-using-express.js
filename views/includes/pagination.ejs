<section class="pagination">
  <% if(currentPage !== 1) { %>
  <a href="?page=1">1</a>
  <% } %> <% if(hasPreviousPage) { %>
  <a href="?page=<%= previousPage %>">Prev</a>
  <% } %>
  <a class="active" href="?page=<%= currentPage %>"><%= currentPage %></a>
  <% if(hasNextPage) { %>
  <a href="?page=<%= nextPage %>">Next</a>
  <% } %> <% if(currentPage !== lastPage) { %>
  <a href="?page=<%= lastPage %>"><%= lastPage %></a>
  <% } %>
</section>
